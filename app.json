{
  "expo": {
    "name": "Relatórios de Ocorrências",
    "slug": "ocorrencias-master-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.luisroyo.ocorrenciasmasterapp",
      "buildNumber": "1.0.1",
      "icon": "./assets/icon.png",
      "infoPlist": {
        "CFBundleDisplayName": "Relatórios de Ocorrências"
      }
    },
    "android": {
      "package": "com.luisroyo.ocorrenciasmasterapp",
      "icon": "./assets/icon.png",
      "label": "Relatórios de Ocorrências",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f5598c2e-0de7-42d0-9439-555a36b86d4d"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "runtimeVersion": "1.0.1",
    "jsEngine": "hermes"
  },
  "cli": {
    "version": ">= 16.17.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}