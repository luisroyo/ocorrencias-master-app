{
  "expo": {
    "name": "Ocorrencias",
    "slug": "ocorrencias-master-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e3a8a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.luisroyo.ocorrenciasmasterapp",
      "infoPlist": {
        "CFBundleDisplayName": "Ocorrencias"
      }
    },
    "android": {
      "package": "com.luisroyo.ocorrenciasmasterapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1e3a8a"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/icon.png",
      "bundler": "metro",
      "output": "static",
      "build": {
        "babel": {
          "include": [
            "@expo/vector-icons"
          ]
        }
      },
      "pwa": {
        "name": "Ocorrencias - Relatórios de Segurança",
        "shortName": "Ocorrencias",
        "description": "Aplicativo para geração de relatórios de ocorrências de segurança",
        "themeColor": "#1e3a8a",
        "backgroundColor": "#ffffff",
        "display": "standalone",
        "orientation": "portrait",
        "scope": "/",
        "startUrl": "/",
        "icons": [
          {
            "src": "./assets/icon.png",
            "sizes": "2048x2048",
            "type": "image/png",
            "purpose": "any maskable"
          }
        ]
      }
    },
    "extra": {
      "eas": {
        "projectId": "f5598c2e-0de7-42d0-9439-555a36b86d4d"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "runtimeVersion": "1.0.1",
    "jsEngine": "hermes",
    "plugins": [
      "expo-router"
    ]
  }
}